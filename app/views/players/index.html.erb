<h2> Player</h2>
<table border='1px'>
<thead >
    <tr>
        <th>Name<th/>
        <th>Date Of Birth<th/>
        <th>Place<th/>
        <th>Email<th/>
        <th>Contact<th/>
        <th>Edit<th/>
        <th>Show</th>
        <th>Destroy</th>
    </tr>
</thead>
<tbody>
    <%= @players.each do |player| %>
    <tr>         
       <td><%= player.name =%></td>
       <td><%= player.date_of_birth =%></td>
       <td><%= player.place =%></td>
       <td><%= player.email =%></td>
       <td><%= player.contact =%></td>
       <td><%= link_to 'Edit', edit_player_path(player) =%></td>
       <td><%= link_to 'Show', player_path(player) =%></td>
       <td><%= link_to "Destroy", player_path(player), data: {
        turbo_method: :delete,
        turbo_confirm: "Are you sure?"
      } %></td>   
    </tr>   
    <% end %>
</tbody> 
</table>   
<br/>
<%= link_to "Add Player", new_player_path %>
     